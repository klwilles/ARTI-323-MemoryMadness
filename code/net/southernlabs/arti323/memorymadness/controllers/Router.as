/** *	Router delegates activities to take place (menu or level) *	Menu or Level take up the entire screen (not parts of it).  */package net.southernlabs.arti323.memorymadness.controllers {		import net.southernlabs.arti323.memorymadness.controllers.places.*;	import net.southernlabs.arti323.memorymadness.controllers.base.Controller;			public class Router extends Controller {						public function Router(place:String) {						if(place=="welcome"){				trace("=> "+place+" context constructed to delegate");				m = new Welcome(); 			} else if(place=="level 1"){				trace("=> "+place+" context constructed to delegate");				l = new Level01(); 			} else if(place=="game over"){				m = new GameOver();			} else {				throw new Error(place+" had no matching implementors for delegate to assign"); 			}					}	}}