package net.southernlabs.arti323.memorymadness.controllers.places {		import flash.display.MovieClip;	import flash.net.URLRequest;	import flash.display.Loader;	import flash.events.Event;	import flash.events.ProgressEvent;		import net.southernlabs.arti323.memorymadness.config.GameConfig;	import net.southernlabs.arti323.memorymadness.controllers.base.LevelInterface;	import net.southernlabs.arti323.memorymadness.views.base.CardInterface;		public class Level01 extends MovieClip implements LevelInterface {				private var c:MovieClip;		var mLoader:Loader = new Loader();		var mRequest:URLRequest = new URLRequest(GameConfig.SWF_PATH+"Level01.swf");		public function Level01() {			// constructor code			trace("Level 01 Created");		}				public function compareCards(card1:CardInterface, card2:CardInterface):Boolean {			return false;		}				public function updateScore():void {					}				public function handleCardClick(item:String):void {			 		}		public function notifyMatch():void {					}				public function setRenderContainer(m:MovieClip):void {			c = m;		}		public function run():void {			c.stage.addEventListener(GameConfig.EVENT_ON_PLAY_GAME, handlePlay);			mLoader.contentLoaderInfo.addEventListener(Event.COMPLETE, onCompleteHandler);			mLoader.load(mRequest);		}		public function end():void {			trace("poof");			c.stage.removeEventListener(GameConfig.EVENT_ON_PLAY_GAME, handlePlay);			mLoader.removeEventListener(Event.COMPLETE, onCompleteHandler);			while( c.numChildren > 0 ){				c.removeChildAt(0);			}			c.router.goto("game-over");		}		function onCompleteHandler(loadEvent:Event) {        	c.addChild(loadEvent.currentTarget.content);		}				function handlePlay(e:Event){			end();		}	}	}