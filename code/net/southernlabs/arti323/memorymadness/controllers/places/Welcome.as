package net.southernlabs.arti323.memorymadness.controllers.places {		import flash.display.MovieClip;	import net.southernlabs.arti323.memorymadness.config.GameConfig;	import net.southernlabs.arti323.memorymadness.controllers.base.MenuInterface;	import net.southernlabs.arti323.memorymadness.views.Preloader;		/*import fl.controls.ProgressBar; 	import fl.controls.ProgressBarMode;	import flash.events.ProgressEvent;	public var pb:ProgressBar = new ProgressBar();	pb.width = 150;	pb.mode = ProgressBarMode.MANUAL;	pb.move(100, 100);	*/	import flash.net.URLRequest;	import flash.display.Loader;	import flash.events.Event;	import flash.events.ProgressEvent;		public class Welcome extends MovieClip implements MenuInterface {		private var c:MovieClip;		var mLoader:Loader = new Loader();		var mRequest:URLRequest = new URLRequest(GameConfig.SWF_PATH+"Welcome.swf");		private var preloader:Preloader;				public function Welcome(){			trace(GameConfig.TITLE);		}				public function handleMenuClick(item:String):void {					}				public function setRenderContainer(m:MovieClip):void {			c = m;		}				public function run():void {			preloader = new Preloader(c, "Welcome");			c.stage.addEventListener(GameConfig.EVENT_ON_PLAY_GAME, handlePlay);			mLoader.contentLoaderInfo.addEventListener(Event.COMPLETE, onCompleteHandler);			mLoader.load(mRequest);		}				public function end():void {			// delete stuff in container			trace("poof");			c.stage.removeEventListener(GameConfig.EVENT_ON_PLAY_GAME, handlePlay);			preloader.end();			mLoader.removeEventListener(Event.COMPLETE, onCompleteHandler);			while( c.numChildren > 0 ){				c.removeChildAt(0);			}			c.router.goto("level-01");		}		function onCompleteHandler(loadEvent:Event) {        	c.addChild(loadEvent.currentTarget.content);		}				function handlePlay(e:Event){			end();		}	}}