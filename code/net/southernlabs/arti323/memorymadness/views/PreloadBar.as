package  net.southernlabs.arti323.memorymadness.views {	import flash.display.MovieClip;		public class PreloadBar extends MovieClip {				private var total_frames:uint = 30;		public function PreloadBar() {			// constructor code		}				public function setProgress(percent:uint):void {			if(percent>=0 && percent <=100){ 				var f:uint = Math.floor(100*(percent/total_frames));				gotoAndPlay(percent);			} else {				// we must be finished				gotoAndStop(total_frames);			}		}	}	}