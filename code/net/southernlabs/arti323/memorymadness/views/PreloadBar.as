package  net.southernlabs.arti323.memorymadness.views {	import flash.display.MovieClip;	import com.inruntime.utils.Global;	import flash.events.Event;	import net.southernlabs.arti323.memorymadness.config.GameConfig;		public class PreloadBar extends MovieClip {				private var total_frames:uint = 17;		private var $:Global = Global.getInstance();		private var place:String = "";		public function PreloadBar() {			// constructor code		}				public function setProgress(percent:uint):void {			if(percent>=0 && percent <=100){ 				var f:uint = Math.floor(100*(percent/total_frames));				gotoAndPlay(percent);			} else {				// we must be finished				play();			}		}				public function setLocation(p:String){			this.place = p;		}				public function notifyFinished(){			stop();			if(place==GameConfig.PLACE_WELCOME){				$.dispatchEvent(new Event(GameConfig.EVENT_ON_WELCOME_LOADED));			} else if(place==GameConfig.PLACE_LEVEL01){				$.dispatchEvent(new Event(GameConfig.EVENT_ON_LEVEL01_LOADED));			} else if(place==GameConfig.PLACE_GAMEOVER){				$.dispatchEvent(new Event(GameConfig.EVENT_ON_GAMEOVER_LOADED));			} else {				// nothing			}					}	}	}